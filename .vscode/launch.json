{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Jest All Tests",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": ["--runInBand", "--detectOpenHandles"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceFolder}"
        }
    ]
}
